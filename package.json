{
  "name": "poc-apollo-v4-graphql",
  "version": "1.0.0",
  "description": "A well-defined GraphQL Modules API using Apollo Server 4",
  "type": "module",
  "scripts": {
    "start": "npm run compile && node --experimental-specifier-resolution=node dist/server.js",
