  "dependencies": {
    "@apollo/server": "^4.10.0",
    "axios": "^1.6.5",
    "graphql": "^16.8.1",
    "graphql-modules": "^2.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/graphql-modules-preset": "^4.0.4",
    "@graphql-codegen/typescript": "^4.0.4",
    "@graphql-codegen/typescript-resolvers": "^4.0.4",
